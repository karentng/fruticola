{# % extends "base.html.twig" % #}



{#% block contenido %#}

<div class="panel panel-default">
    <div class="panel-heading">
        <div class="icon"><i class="icon20 i-menu-6"></i></div>
        <h4>A1. Datos Personales</h4>
        <a href="#" class="minimize"></a>
    </div><!-- End .panel-heading -->

    <div class="panel-body" style="display: block;">
        <div class="container"> 
            <form class="form-horizontal" role="form">
                <div class="row">
                    <div class="col-md-3">
                        <div class="container">
                            <div class="form-group">
                                <label class="control-label">Primer Nombre</label>
                                <input type="text" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="container">
                            <div class="form-group">
                                <label class="control-label">Segundo Nombre</label>
                                <input type="text" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="container">
                            <div class="form-group">
                                <label class="control-label">Primer Apellido</label>
                                <input type="text" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="container">
                            <div class="form-group">
                                <label class="control-label">Segundo Apellido</label>
                                <input type="text" class="form-control" />
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div div class="col-md-6">
                        <div class="form-group">
                            <label class="col-xs-3 control-label">Documento: </label>
                            <div class="col-xs-3" style="padding-right:0px !important">
                                <select class="form-control">
                                    <option>C.C.</option>
                                    <option>T.I.</option>
                                    <option>C.E.</option>
                                    <option>NIT</option>
                                </select>
                            </div>
                            <div class="col-xs-6" style="padding-left:0px !important">
                                <input class="form-control" type="text" placeholder="Número">
                            </div>
                        </div>
                    </div>
                    
                    <div div class="col-md-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label" for="fechaNacimiento">Fecha de Nacimiento:</label>
                            <div class="col-sm-6">
                                <div class="input-group date datepicker autostart">
                                    <input size="16" name="fechaNacimiento" class="form-control" type="text">
                                    <span class="input-group-addon"><i class="icon16 i-calendar-4"></i></span>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                </div>
                <div class="row">
                    <div div class="col-md-4">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">Sexo: </label>
                            <div class="col-sm-6">
                                <select class="form-control">
                                    <option>Hombre</option>
                                    <option>Mujer</option>
                                </select>
                            </div>
                        </div>
                    </div>                    
                    <div div class="col-md-4">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">Nivel Educativo: </label>
                            <div class="col-sm-6">
                                <select class="form-control">
                                    <option></option>
                                    <option value="1">Primaria</option>
                                    <option value="2">Secundaria</option>
                                    <option value="3">Técnica</option>
                                    <option value="4">Tecnológica</option>
                                    <option value="5">Universitaria</option>
                                    <option value="6">Ninguna</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    
                </div>
                
                <div class="row">
                    <div div class="col-md-6">
                        <div class="form-group">
                            <label class="col-sm-4 control-label" for="focused">Tipo de Productor:</label>
                            <div class="col-sm-8">
                                <input class="form-control" id="focusedInput" type="text" placeholder="Tipo de Productor Según la Clasificación FINAGRO" >
                            </div>
                        </div>
                    </div>
                    <div div class="col-md-6">
                        <div class="form-group">
                            <label class="col-sm-4 control-label" for="focused">Cadena Productiva:</label>
                            <div class="col-sm-8">
                                <input class="form-control" id="focusedInput" type="text" placeholder="Cadena Productiva FRUTÍCOLA / Producto seleccionado" >
                            </div>
                        </div>
                    </div>
                </div>


            </form>
        </div> <!-- .container -->
    </div><!-- End .panel-body -->

</div><!-- panel default -->
